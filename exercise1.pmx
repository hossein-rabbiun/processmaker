        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// Trigger to validate Iranian National Code in ProcessMaker

// Replace 'form[national_code]' with the variable name of your national code field in ProcessMaker
var nationalCode = @@txt_personalCode;

function isValidIranianNationalCode(code) {
    // Check if the input is a 10-digit number
    if (!/^\d{10}$/.test(code)) return false;

    // Extract the check digit
    const check = +code[9];

    // Calculate the checksum
    const sum = Array.from(code.substring(0, 9)).reduce((acc, digit, index) => {
        return acc + (+digit * (10 - index));
    }, 0);

    // Validate based on the checksum rules
    const remainder = sum % 11;
    return (remainder < 2 && check === remainder) || (remainder >= 2 && check === 11 - remainder);
}

if (!isValidIranianNationalCode(nationalCode)) {
    // Validation failed: Display error and stop the process
    throw "کد ملی وارد شده معتبر نیست. لطفاً مجدداً بررسی کنید.";
}
]]></tri_webbot>
