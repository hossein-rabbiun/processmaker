        <dyn_content><![CDATA[{"name":"fill_form","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"72321366967383cce387c03058240915","name":"fill_form","description":"","mode":"edit","script":{"type":"js","code":"// انتخاب دکمه \"ثبت\" با نوع submit\nconst submitButton = document.querySelector('button[type=\"submit\"]');\n\n// بررسی وجود دکمه و اعمال استایل\nif (submitButton) {\n    submitButton.style.backgroundColor = '#4CAF50'; // رنگ سبز\n    submitButton.style.color = 'white'; // رنگ متن سفید\n    submitButton.style.border = 'none'; // حذف حاشیه\n    submitButton.style.padding = '10px 20px'; // فاصله داخلی\n    submitButton.style.fontSize = '16px'; // اندازه فونت\n    submitButton.style.borderRadius = '5px'; // گوشه‌های گرد\n    submitButton.style.cursor = 'pointer'; // تغییر نشانگر موس\n}\n\n// امکان باز و بسته کردن پنل\n$(document).ready(function () {\n      $(\"#pnl_01\").on(\"click\", function () {\n        // باز و بسته کردن پنل\n        $(\"#txt_nationalCode, #tex_fullname, #tex_faderName, #dat_birthday,#tex_shenasnameh_code, #tex_birthCity, #tex_tellNumber,#tex_mobil,#tex_shenasnameh_code\").slideToggle(\"fast\");\n\n    });\n    // اضافه کردن رویداد کلیک به پنل با آیدی pnl_02\n    $(\"#pnl_02\").on(\"click\", function () {\n        // باز و بسته کردن پنل\n        $(\"#tex_personalCodeNemayandeh, #tex_fullNameNemayandeh, #tex_tellNumberNemayandeh, #tex_mobileNemayandeh\").slideToggle(\"fast\");\n\n    });\n});\n/*\n$(document).ready(function () {\n    $(\"#sbt_01\").on(\"click\", function (e) {\n        e.preventDefault(); // جلوگیری از ارسال پیش‌فرض فرم\n\n        const nationalCode = $(\"#txt_nationalCode\").getValue(); \n      const fullname = $(\"#tex_fullname\").getValue(); \n\t\t$.ajax({\n   \t\t\t url: \"http://localhost/sysworkflow/en/neoclassic/90093975067383acc7add76075727389/45164525367383b466f09d8003125499.php\", // Endpoint to call the trigger\n   \t\t\t type: \"POST\",\n   \t\t\t data: { txt_nationalCode:$(\"#txt_nationalCode\").getValue() },\n    \t\t success: function (response) {\n               //  if (response == \"2\") {\n\n      \t\t\t  if ( $(\"#txt_nationalCode\").getValue() == 4270792965  ) {\n        \t\t      alert(\"این کد ملی قبلاً ثبت شده است.\");\n       \t\t\t   } else {\n                    // باز کردن صفحه جدید و ارسال اطلاعات\n                    var newWindow = window.open(\"\", \"_blank\"); // باز کردن صفحه جدید\n                    newWindow.document.write(`\n                        <html>\n                            <head>\n                                <title>اطلاعات کاربر</title>\n                            </head>\n                            <body>\n                                <h1>اطلاعات وارد شده:</h1>\n                                <p><strong>کد ملی:</strong> ${nationalCode}</p>\n                                <p><strong>نام و نام خانوادگی:</strong> ${fullname}</p>\n                                         <a href=\"http://localhost/sysworkflow/en/neoclassic/90093975067383acc7add76075727389/45164525367383b466f09d8003125499.php\"\n                                                style=\"display: inline-block; margin-top: 20px; background-color: #4CAF50; color: white; text-decoration: none; padding: 10px 20px; border-radius: 5px;\">\n                                                      بازگشت به صفحه قبل\n                                          </a>\n                            </body>\n                        </html>\n                    `);\n                    newWindow.document.close(); // بستن جریان نوشتن به صفحه\n                }\n   \t\t\t },\n            error: function () {\n     \t\t\t   alert(\"خطایی در سرور رخ داده است.\");\n    }\n});\n\n    });\n});\n*/\n\n\n\n$(document).ready(function () {\n    $(\"#sbt_01\").on(\"click\", function (e) {\n        e.preventDefault(); // جلوگیری از ارسال پیش‌فرض فرم\n        const nationalCode = $(\"#txt_nationalCode\").getValue(); \n    \t\t$.ajax({\n   \t\t\t url: \"http://localhost/sysworkflow/en/neoclassic/90093975067383acc7add76075727389/45164525367383b466f09d8003125499.php\", // Endpoint to call the trigger\n              type: \"POST\",\n              data: { txt_nationalCode:nationalCode },\n    \t\t success: function (response) {\n                // if (response.isDuplicate === \"true\") {\n\n      \t\t\t  if (nationalCode == \"4270792965\" ) {\n        \t\t      alert(\"این کد ملی قبلاً ثبت شده است.\");\n       \t\t\t   } else {\n                      alert(\" فرایند موفق\");\n                     $(\"#72321366967383cce387c03058240915\").submit(); \n                    // باز کردن صفحه جدید و ارسال اطلاعات\n\t\t\t\t\t\t\n               }\n   \t\t\t },\n            error: function () {\n     \t\t\t   alert(\"خطایی در سرور رخ داده است.\");\n    }\n});\n\n    });\n});\n\n\n\n\n\n\n\n\n\n// صحت سنجی کد ملی\n$(document).ready(function () {\n    // اضافه کردن رویداد focusout به فیلد کد ملی\n    $(\"#txt_nationalCode\").on(\"focusout\", function () {\n        const nationalCode = $(this).getValue();  \n        let isValid = true;\n\n            let sum = 0;\n            for (let i = 0; i < 9; i++) {\n                sum += parseInt(nationalCode.charAt(i)) * (10 - i);\n            }\n\n            const lastDigit = parseInt(nationalCode.charAt(9));\n            const divideRemaining = sum % 11;\n\n            if (divideRemaining < 2) {\n                isValid = (lastDigit == divideRemaining);\n            } else {\n                isValid = (lastDigit == (11 - divideRemaining));\n            }\n        \n        if (!isValid) {\n            $(this).css(\"border\", \"2px solid red\"); // تغییر حاشیه به رنگ قرمز در صورت عدم صحت کد ملی\n            alert(\"کد ملی وارد شده معتبر نمی‌باشد.\");}\n          else{\n             $(this).css(\"border\", \"1px solid #ccc\");    \n        }\n    });\n});\n\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"colSpan":3},{"colSpan":3},{"colSpan":3},{"type":"radio","variable":"reque