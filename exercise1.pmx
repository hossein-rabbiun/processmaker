        <tri_uid>266837428674f029c0c09d8035113032</tri_uid>
        <tri_title>test_creatUser</tri_title>
        <tri_description></tri_description>
        <pro_uid>90093975067383acc7add76075727389</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[
$nationalCode = @@txt_nationalCode; // دریافت کد ملی از فرم
$mobile = @@tex_mobil;             // دریافت شماره موبایل از فرم
$fullName = @@text_fullname;       // دریافت نام و نام خانوادگی از فرم
// بررسی وجود کد ملی در پایگاه داده
$query = "SELECT COUNT(*) AS count FROM USER WHERE USR_USERNAME = ?";
$result = executeQuery($query, [$nationalCode]);

if ($result && $result[1]['count'] > 0) {
    // کد ملی قبلاً ثبت شده است
    @@errorMessage = "این کد ملی قبلاً ثبت شده است.";
} else {
    // ایجاد کاربر جدید
    $username = $nationalCode; // استفاده از کد ملی به عنوان یوزرنیم
    @%result = PMFCreateUser($username, $mobile, $fullName, $fullName, 'hr@example.com',
        'PROCESSMAKER_OPERATOR', '2018-12-31', 'ACTIVE');
    if (@%result) {
        require_once 'classes/model/Users.php';
        $u = new Users();
        $aUser = $u->loadByUsernameInArray($username);
        $aUser = $u->load($aUser['USR_UID']);
 
        $aReplaced = $u->loadByUsernameInArray('dsmith');
        $aUser['USR_REPLACED_BY'] = $aReplaced['USR_UID'];
 
        @@res = $u->create($aUser);
    } else {
        // خطا در ایجاد کاربر
        @@errorMessage = "خطا در ایجاد کاربر. لطفاً دوباره تلاش کنید.";$errorMessage=@@errorMessage; @@hiden=$errorMessage;

    }
}
]]></tri_webbot>
