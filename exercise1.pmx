        <dyn_content><![CDATA[{"name":"test_code","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"942361191673e225ea3d9b9071999633","name":"test_code","description":"","mode":"edit","script":{"type":"js","code":"/*\n$(document).ready(function () {\n    $(\"#submit01\").click(function (event) {\n        // جلوگیری از ارسال پیش‌فرض فرم\n        event.preventDefault();\n\n        // مقادیر فیلدها را دریافت کنید\n        let nationalCode = $(\"#txt1\").val();\n        let fullName = $(\"#txt2\").val();\n        // محتوای صفحه جدید\n        let newPageContent = `\n            <html>\n                <head>\n                    <title>اطلاعات وارد شده</title>\n                    <style>\n                        body {\n                            font-family: Arial,  sans-serif;\n                            margin: 20px;\n                            padding: 20px;\n                            background-color: #f9f9f9;\n                        }\n                        .info-box {\n                            border: 1px solid #ccc;\n                            border-radius: 5px;\n                            padding: 10px;\n                            background-color: #fff;\n                            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n                        }\n                        .info-box h3 {\n                            margin-bottom: 10px;\n                            color: #333;\n                        }\n                        .info-box p {\n                            margin: 5px 0;\n                        }\n                    </style>\n                </head>\n                <body>\n                    <div class=\"info-box\">\n                        <h3>اطلاعات شما</h3>\n                        <p><strong>کد ملی:</strong> ${nationalCode} </p>\n                        <p><strong>نام و نام خانوادگی:</strong> ${fullName}</p>\n                    </div>\n                </body>\n            </html>\n        `;\n\n        // باز کردن تب جدید و نوشتن محتوا در آن\n        let newWindow = window.open(\"\", \"_blank\");\n        newWindow.document.open();\n        newWindow.document.write(newPageContent);\n        newWindow.document.close();\n    });\n});\n\n$(document).ready(function () {\n    $(\"#s1\").click(function () {\n        var txt1Value = $(\"#txt1\").val();\n        var txt2Value = $(\"#txt2\").val();\n\n        // ایجاد یک تب جدید و نوشتن مقادیر در آن\n        var newTab = window.open();\n        newTab.document.write(\"<html><head><title>نمایش اطلاعات</title></head><body>\");\n        newTab.document.write(\"<h1>مقادیر وارد شده</h1>\");\n        newTab.document.write(\"<p>مقدار 1: \" + txt1Value + \"</p>\");\n        newTab.document.write(\"<p>مقدار 2: \" + txt2Value + \"</p>\");\n        newTab.document.write(\"</body></html>\");\n        newTab.document.close();\n    });\n});\n*/\n$(document).ready(function () {\n    $(\"#txt1\").on(\"focusout\", function () {\n        var codeLength = $(this).val().length;\n        if (codeLength !== 2) {\n            $(this).css(\"border\", \"2px solid red\");\n            alert(\"طول کد ملی باید دقیقاً 2 کاراکتر باشد.\");\n        } else {\n            $(this).css(\"border\", \"1px solid #ccc\");\n        }\n    });\n});\n\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"text","variable":"txt2","var_uid":"391322455673e22d766f921007473028","dataType":"string","protectedValue":false,"id":"txt2","name":"txt2","label":"Txt 2","tabIndex":"","defaultValue":"","placeholder":"","hint":"","ariaLabel":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","memoryCache":false,"var_name":"txt2","colSpan":6},{"type":"text","variable":"txt1","var_uid":"542051429673e228b632e84059183500","dataType":"string","protectedValue":false,"id":"txt1","name":"txt1","label":"Txt 1","tabIndex":"","defaultValue":"","placeholder":"","hint":"","ariaLabel":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","memoryCache":false,"var_name":"txt1","colSpan":6}],[{"type":"submit","id":"s1","name":"submit0000000001","label":"submit_1","tabIndex":"","ariaLabel":"","colSpan":12}]],"variables":[{"var_uid":"391322455673e22d766f921007473028","prj_uid":"90093975067383acc7add76075727389","var_name":"txt2","var_field_type":"string","var_field_size":10,"var_label":"string","var_dbconnection":"workflow","var_dbconnection_label":"PM Database","var_sql":"","var_null":0,"var_default":"","var_accepted_values":"[]","inp_doc_uid":""},{"var_uid":"542051429673e228b632e84059183500","prj_uid":"90093975067383acc7add76075727389","var_name":"txt1","var_field_type":"string","var_field_size":10,"var_label":"string","var_dbconnection":"workflow","var_dbconnection_label":"PM Database","var_sql":"","var_null":0,"var_default":"","var_accepted_values":"[]","inp_doc_uid":""}]}]}]]></dyn_content>
